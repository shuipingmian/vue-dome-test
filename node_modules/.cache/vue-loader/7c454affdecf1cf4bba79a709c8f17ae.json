is component only -->\n<style lang=\"scss\" scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n.dw-left__menu {\n  flex: 1;\n  background: #111112;\n  color: rgba(255, 255, 255, 0.6);\n  line-height: 40px;\n  overflow-y: auto;\n  border: none;\n\n  .menu-icon {\n    font-size: 18px !important;\n    color: rgba(255, 255, 255, 0.6);\n    width: 18px;\n    display: inline-block;\n    color: rgba(255, 255, 255, 0.6);\n  }\n\n  .el-submenu.is-active { // 选中的高亮\n    >.el-submenu__title { // 栏目的高亮\n      .iconfont, .dw-menuItem_title {\n        color: #fff;\n      }\n    }\n    .el-menu { // 页面高亮\n      >.el-menu-item.is-active {\n        .dw-menuItem_text {\n          color: #fff;\n        }\n      }\n    }\n  }\n  .dw-menu__li:hover{\n    .dw-menuItem_text {\n      color: rgba(63, 125, 197, 1) !important;\n    }\n  }\n\n  .dw-menu__item {\n    .el-submenu__title {\n      // 一级菜单\n      padding: 0 12px !important;\n      height: 40px;\n      line-height: 40px;\n      font-size: 14px;\n\n      font-weight: 400;\n      color: rgba(255, 255, 255, 0.6);\n\n      &:hover {\n        background: rgba(34, 41, 48, 0.7);\n        color: $darkBlueColor;\n\n        .iconfont {\n          color: #FFFFFF;\n        }\n      }\n\n    }\n\n    .el-menu--inline {\n      background: #111112;\n\n      .dw-menuItem_text {\n        height: 40px;\n        line-height: 40px;\n        min-width: auto;\n        font-size: 14px;\n\n        font-weight: 400;\n        color: rgba(255, 255, 255, 0.6);\n\n        &.is-active {\n          color: #fff;\n          background: rgba(71, 121, 244, .5) !important;\n        }\n\n        &:hover {\n          color: rgba(63, 125, 197, 1) !important;\n          background: rgba(34, 41, 48, 0.7) !important\n        }\n      }\n    }\n  }\n\n  .dw-menu__li {\n    padding: 0 12px !important;\n    height: 40px;\n    line-height: 40px;\n    font-size: 14px;\n\n    font-weight: 400;\n    color: rgba(255, 255, 255, 0.6);\n\n    &.is-active {\n      background: rgba(71, 121, 244, .5);\n      color: #FFFFFF;\n\n      i {\n        color: #fff;\n      }\n    }\n\n    &:hover {\n      background: rgba(34, 41, 48, 0.7);\n      color: $darkBlueColor;\n\n      .iconfont {\n        color: #FFFFFF;\n      }\n    }\n  }\n\n  &.el-menu--collapse {\n    width: 48px;\n\n    .dw-menu__item.is-active {\n      background: rgba(71, 121, 244, .5);\n\n      .el-submenu__title i {\n        color: #fff;\n      }\n    }\n\n    .dw-menu__li.is-active {\n      background: rgba(71, 121, 244, .5);\n      i {\n        color: #fff;\n      }\n    }\n  }\n\n}\n\n</style>\n"]}]}